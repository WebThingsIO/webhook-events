{
  "author": "WebThingsIO",
  "content_scripts": [
    {
      "css": [
        "css/extension.css"
      ],
      "js": [
        "js/extension.js"
      ]
    }
  ],
  "description": "Webhook events add-on for WebThings Gateway",
  "gateway_specific_settings": {
    "webthings": {
      "exec": "{nodeLoader} {path}",
      "primary_type": "extension",
      "strict_max_version": "*",
      "strict_min_version": "0.10.0"
    }
  },
  "homepage_url": "https://github.com/WebThingsIO/webhook-events",
  "id": "webhook-events",
  "license": "MPL-2.0",
  "manifest_version": 1,
  "name": "Webhook Events",
  "options": {
    "default": {
      "events": []
    },
    "schema": {
      "type": "object",
      "properties": {
        "events": {
          "type": "array",
          "title": "Events",
          "description": "List of event names that can be generated",
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "short_name": "Webhook",
  "version": "0.0.1",
  "web_accessible_resources": [
    "css/*.css",
    "js/*.js",
    "views/*.html"
  ]
}
